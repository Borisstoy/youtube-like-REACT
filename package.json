{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/StephenGrider/ReduxSimpleStarter.git"
  },
  "scripts": {
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
    "postinstall": "webpack -p",
    "start": "node server.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p",
    "heroku-postbuild": "next build"
  },
  "author": "",
