    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack-dev-server": "^1.14.0",
    "react-hot-loader": "^1.3.0"
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
